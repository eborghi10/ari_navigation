<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="serial_no" />
  <arg name="device_type"       default="t265"/>
  <arg name="camera_name"       value="torso_back_camera"/>
  <arg name="tf_prefix_camera"  default="$(arg camera_name)"/>
  <arg name="enable_fisheye"    default="true"/>
  <arg name="initial_reset"     default="false"/>
  <arg name="topic_odom_in"     default="odom_in"/>
  <arg name="calib_odom_file"   default=""/>
  <arg name="publish_odom_tf"   default="false"/>

  <group ns="$(arg camera_name)">
    <include file="$(find realsense2_camera)/launch/includes/nodelet.launch.xml">
      <arg name="device_type"           value="$(arg device_type)"/>
      <arg name="serial_no"             value="$(arg serial_no)"/>
      <arg name="tf_prefix"             value="$(arg tf_prefix_camera)"/>
      <arg name="enable_fisheye1"       value="$(arg enable_fisheye)"/>
      <arg name="enable_fisheye2"       value="$(arg enable_fisheye)"/>
      <arg name="initial_reset"         value="$(arg initial_reset)"/>
      <arg name="topic_odom_in"         value="$(arg topic_odom_in)"/>
      <arg name="calib_odom_file"       value="$(arg calib_odom_file)"/>
      <arg name="publish_odom_tf"       value="$(arg publish_odom_tf)"/>
    </include>
  </group>

  <node pkg="tf" type="static_transform_publisher" name="torso_camera_base_link_broadcaster" args="-0.3275 0.009 1.0337 3.14 0.26 0.0 base_link torso_back_camera_pose_frame 100" />:

</launch>
